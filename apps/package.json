  "name": "andtopic-app",
  "version": "0.0.13",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "NODE_ENV=development react-router dev",
    "hocuspocus": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./websocket/server.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "preview": "vite preview",
    "typecheck": "react-router typegen && tsc -p tsconfig.typecheck.json",
    "typegen": "react-router typegen",
    "typegen-watch": "react-router typegen --watch",
    "dev:host": "npm run dev -- --host 0.0.0.0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "node ./scripts/convert-imports.mjs && npx prettier --write ./{websocket,app,tests}/**/*.{ts,tsx}",
    "format-check": "npx prettier --check ./{app,tests}/**/*.{ts,tsx}",
    "test": "vitest",
    "codegen": "mkdir -p ./app/nest-client/__generated__ && openapi-zod-client ./app/nest-client/__generated__/openapi.json -o ./app/nest-client/api-client.ts && node --loader ts-node/esm codegen.ts",
    "jwtgen": "node --loader ts-node/esm jwtgen.ts",
    "start": "react-router-serve ./build/server/index.js"
  },
